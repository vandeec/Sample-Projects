<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.ogurycordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ogury Cordova</name>
    <description>
        A sample Ogury App built in Cordova.
    </description>
    <author email="loic@ogury.co" href="http://www.ogury.com">
        Ogury Tech Ops Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="Presage" spec="/Users/loicvdb/Downloads/presage-cordova-3.0.18-3.0.12" />
    <engine name="android" spec="^7.0.0" />
</widget>
